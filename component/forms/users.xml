<form>
    <field name="username">
      <type>text</type>
      <name>username</name>
      <id>username</id>
      <label>FOXY_SETTINGS_USERNAME_LABEL</label>
      <message>FOXY_SETTINGS_USERNAME_MSG</message>
      <placeholder>GW_SETTINGS_USERNAME</placeholder>
      <disabled>false</disabled>
    </field>
    <field name="email">
      <type>text</type>
      <name>email</name>
      <id>email</id>
      <label>FOXY_SETTINGS_EMAIL_LABEL</label>
      <message>Invalid email format</message>
      <placeholder>FOXY_SETTINGS_EMAIL</placeholder>
      <disabled>false</disabled>
    </field>
    <field name="password">
      <type>password</type>
      <name>password</name>
      <id>password</id>
      <label>FOXY_SETTINGS_PASSWORD_LABEL</label>
      <message></message>
      <placeholder>FOXY_SETTINGS_PASSWORD</placeholder>
      <onchange>checkPassword();</onchange>
      <disabled>false</disabled>
    </field>
    <field name="usergroup">
      <type>usergroup</type>
      <name>usergroup</name>
      <id>usergroup</id>
      <label>Usergroup</label>
    </field>
</form>